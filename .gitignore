# dependencies
node_modules
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# testing
/coverage

# next.js build output (root app + workspace apps)
/.next/
/out/
apps/*/.next/
apps/*/out/

# production
/build
apps/*/build

# misc
.DS_Store
*.pem

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# env files (commit .env.example but ignore .env)
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
!.env.example
apps/**/.env
apps/**/.env.local
!apps/**/.env.example

# vercel
.vercel

# typescript
*.tsbuildinfo
next-env.d.ts

# prisma generated client (root and packages)
/src/generated/prisma
packages/db/src/generated/prisma
/prisma/*.db
/prisma/*.db-journal
/prisma/*.db-shm
/prisma/*.db-wal
packages/db/prisma/*.db

# reference data (large files)
/.reference-files/*.json
/.reference-files/*.geojson
