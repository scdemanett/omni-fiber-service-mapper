{
  "name": "fiber-service-mapper",
  "version": "2.6.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:admin": "npm run dev --workspace=apps/admin",
    "dev:public": "npm run dev --workspace=apps/public",
    "build:admin": "npm run build --workspace=apps/admin",
    "build:public": "npm run generate --workspace=packages/db && npm run build --workspace=apps/public"
  }
}
